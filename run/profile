spl () { docker run --rm -v `printf "%q" "$1"`:/root albumsplitter; }
c () { VSCODE_CWD="$PWD" open -n -b "com.microsoft.VSCode" --args $* ;}
dcli () { docker images |grep none|awk {'print $3 '} |xargs docker rmi ;}
dcl () { docker ps -a |grep -v CONTAINER|awk {' print $1 '} |xargs docker rm;}

